{
    "views" : {
        "all" : {
           "map": "function(doc) { emit(doc._id, doc); }"
        },
        "name": {
           "map": "function(doc) { emit(doc.OLProjectNameKey, doc); }"
        },
        "userIdentifier": {
           "map": "function(doc) { emit(doc.OLProjectUserKey, doc); }"
        },
        "all_by_project_name": {
           "map": "function(doc) { emit(doc._id, {'id':doc._id, 'name':doc.OLProjectNameKey}); }"
        }
    }
}